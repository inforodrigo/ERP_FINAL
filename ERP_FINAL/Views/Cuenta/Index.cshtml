@section Scripts{

    <script src="@Url.Content("~/Content/arbol/js/gijgo.min.js")"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script src="~/Front/plugins/select2/js/select2.full.min.js"></script>


    <script type="text/javascript">

        let tree;

        $(document).ready(function () {
            listarCuenta();
        });

        let idCuenta = 0;
        let idPadre = 1;
        let NombreCuenta = "";

        function listarCuenta() {
            $.ajax({
                url: "@Url.Action("ListarCuenta", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "json",
                        data: {


                }, success: function (result) {
                                tree = $('#tree').tree({
                                        primaryKey: 'Id',
                                        dataSource: result.Cuentas,
                                        uiLibrary: 'bootstrap4',
                                        select: function (e, node, id) {
                                            idCuenta = id;
                                            idPadre = 0;
                                        },
                                        cascadeSelection: false,
                                });
                            tree.expandAll()
                                    tree.on('dataBound', function () {
                                        var node = tree.getNodeByText('North America');
                                        tree.expand(node);
                                    });
                                tree.on('unselect', function (e, node, id) {
                                    idCuenta = 0;
                                    idPadre = 1;
                                });
                }

            });
        }

        function RegistroCuenta() {
            $.ajax({
                url: "@Url.Action("Create", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "script",
                        data: {
                            nombre: $("#itNombre").val(),
                            idCuenta: idCuenta,
                            idPadre: idPadre

                }, success: function () {
                            //idCuenta = 0;
                            //idPadre = 1;
                }
            });
        }

        function ObtenerCuenta() {

            if (idCuenta == 0) {
                    return MostrarMensaje("Debe seleccionar una cuenta");
            }

            $.ajax({
                url: "@Url.Action("ObtenerCuenta", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "json",
                data: {
                    idcuenta: idCuenta

                }, success: function (result) {
                    $("#tituloeditar").html("Editar Cuenta");
                    $("#itNombreEditar").val(result.Nombre);
                    $("#modalEditar").modal('show');
                }

                });
        }

        function ModalEliminarCuenta() {

                if (idCuenta == 0) {
                       return MostrarMensaje("Debe seleccionar una cuenta");
                }

                $.ajax({
                    url: "@Url.Action("ObtenerCuenta", "Cuenta")",
                    cache: false,
                            type: "POST",
                            dataType: "json",
                    data: {
                        idcuenta: idCuenta

                    }, success: function (result) {

                        var mesjModalEliminar = "¿Esta seguro que deseas eliminar la cuenta " + result.Nombre + " ?";
                        $("#texto").html(mesjModalEliminar);
                        $("#modalEliminar").modal('show');
                    }
                 });
        }

        function EditarCuenta() {
            if (idCuenta == 0) {
                    return MostrarMensaje("Debe seleccionar una cuenta");
            }

            $.ajax({
                url: "@Url.Action("Edit", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "script",
                        data: {
                            idCuenta: idCuenta,
                            nombre:  $("#itNombreEditar").val()

                }, success: function () {
                    idCuenta = 0;
                    idPadre = 1;
                }
            });

        }

        function EliminarCuenta() {
            if (idCuenta == 0) {
                    return MostrarMensaje("Debe seleccionar una cuenta");
            }

            $.ajax({
                url: "@Url.Action("Delete", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "script",
                        data: {
                            idCuenta: idCuenta


                }, success: function () {

                }
            });

        }

        var Id = null;

        function AbrirModalEliminar(id, nombre) {

            Id = id;
            if (Id == null) {
                return MostrarMensaje("Debe seleccionar la cuenta");
            }

            var mesjModalEliminar = "¿Esta seguro que deseas eliminar la cuenta " + nombre + " ?";
            $("#texto").html(mesjModalEliminar);
            $("#modalEliminar").modal('show');
        }

        function Eliminar() {
            if ( Id== null) {
                    return MostrarMensaje("Debe seleccionar la cuenta");
            }

            $.ajax({
                    url: "@Url.Action("Delete", "Cuenta")",
                    cache: false,
                            type: "POST",
                            dataType: "script",
                data: {
                    id: Id
                }

            });
        }

        function AbrirModalConfiguracion() {

            $.ajax({
                url: "@Url.Action("VerificarConfiguracion", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "json",
                data: {
                    id: 0

                }, success: function (result) {
                    var conf = result.integracion;
                    if (conf == 0) {
                        $('#modalconfiguracion').modal('show');
                        ObtenerCuentasConfiguracion();
                    } else {
                        $('#modalconfiguracionedit').modal('show');
                        ObtenerConfiguracion();
                    }
                }
            });
        }

        function ObtenerConfiguracion() {
            var si = document.getElementById('sied');
            var no = document.getElementById('noed');
            $.ajax({
                url: "@Url.Action("ObtenerConfiguracion", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "json",
                data: {
                    id: 0

                }, success: function (result) {
                    console.log(result);
                    var conf = result.integracion;
                    if (conf == 0) {
                        no.checked = true;
                    } else {
                        si.checked = true;
                    }
                    $("#cajaed").val(result.caja);
                    $("#creditofiscaled").val(result.creditofiscal);
                    $("#debitofiscaled").val(result.debitofiscal);
                    $("#comprased").val(result.compras);
                    $("#ventased").val(result.ventas);
                    $("#ited").val(result.it);
                    $("#itxpagared").val(result.itxpagar);
                }
            });
        }

        function ObtenerCuentasConfiguracion() {
            $("#caja").empty();
            $("#creditofiscal").empty();
            $("#debitofiscal").empty();
            $("#compras").empty();
            $("#ventas").empty();
            $("#it").empty();
            $("#itxpagar").empty();
            $.ajax({
                url: "@Url.Action("ObtenerCuentasConfiguracion", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "json",
                data: {
                    id: 0

                }, success: function (result) {
                    $.each(result.cuentas, function (index, value) {
                        $('#caja').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#caja').select2({
                            theme: "classic"
                        });
                        $('#creditofiscal').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#creditofiscal').select2({
                            theme: "classic"
                        });
                        $('#debitofiscal').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#debitofiscal').select2({
                            theme: "classic"
                        });
                        $('#compras').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#compras').select2({
                            theme: "classic"
                        });
                        $('#ventas').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#ventas').select2({
                            theme: "classic"
                        });
                        $('#it').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#it').select2({
                            theme: "classic"
                        });
                        $('#itxpagar').append('<option value=' + value.Id + ' > ' + value.Codigo + ' - ' + value.Nombre + '</option>');
                        $('#itxpagar').select2({
                            theme: "classic"
                        });
                    });
                }
            });
        }

        function validarCuentas() {
            if ($("#caja").val() == $("#creditofiscal").val() || $("#caja").val() == $("#debitofiscal").val() || $("#caja").val() == $("#compras").val() || $("#caja").val() == $("#ventas").val() || $("#caja").val() == $("#it").val() || $("#caja").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#creditofiscal").val() == $("#caja").val() || $("#creditofiscal").val() == $("#debitofiscal").val() || $("#creditofiscal").val() == $("#compras").val() || $("#creditofiscal").val() == $("#ventas").val() || $("#creditofiscal").val() == $("#it").val() || $("#creditofiscal").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#debitofiscal").val() == $("#caja").val() || $("#debitofiscal").val() == $("#creditofiscal").val() || $("#debitofiscal").val() == $("#compras").val() || $("#debitofiscal").val() == $("#ventas").val() || $("#debitofiscal").val() == $("#it").val() || $("#debitofiscal").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#compras").val() == $("#caja").val() || $("#compras").val() == $("#creditofiscal").val() || $("#compras").val() == $("#debitofiscal").val() || $("#compras").val() == $("#ventas").val() || $("#compras").val() == $("#it").val() || $("#compras").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#ventas").val() == $("#caja").val() || $("#ventas").val() == $("#creditofiscal").val() || $("#ventas").val() == $("#debitofiscal").val() || $("#ventas").val() == $("#compras").val() || $("#ventas").val() == $("#it").val() || $("#ventas").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#it").val() == $("#caja").val() || $("#it").val() == $("#creditofiscal").val() || $("#it").val() == $("#debitofiscal").val() || $("#it").val() == $("#compras").val() || $("#it").val() == $("#ventas").val() || $("#it").val() == $("#itxpagar").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            if ($("#itxpagar").val() == $("#caja").val() || $("#itxpagar").val() == $("#creditofiscal").val() || $("#itxpagar").val() == $("#debitofiscal").val() || $("#itxpagar").val() == $("#compras").val() || $("#itxpagar").val() == $("#ventas").val() || $("#itxpagar").val() == $("#it").val()) {
                return MostrarMensaje('Error no se puede seleccionar mas de una vez una sola cuenta');
                return false;
            }
            return true;
        }

        function RegistrarConfiguracion() {
            var configuracion = $("input[name='integracion']:checked").val();
            var validate = validarCuentas();
            if (!validate) {
                return;
            }


            $.ajax({
                url: "@Url.Action("RegistrarConfiguracion", "Cuenta")",
                cache: false,
                        type: "POST",
                        dataType: "script",
                data: {
                    configuracion: configuracion, caja: $("#caja").val(), creditofiscal: $("#creditofiscal").val(),
                    debitofiscal: $("#debitofiscal").val(), compras: $("#compras").val(), ventas: $("#ventas").val(),
                    it: $("#it").val(), itxpagar: $("#itxpagar").val()
                }
            });
            $("#modalconfiguracion").modal('hide');
        }

