
@{
    ViewBag.Title = "Index";
}

@section EtiquetaHead
{
    <link href="@Url.Content("~/Content/arbol/css/gijgo.min.css")" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.11.5/css/jquery.dataTables.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <link href="@Url.Content("~/Front/plugins/select2/css/select2.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Front/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css")" rel="stylesheet" />
}

<h2>Plan de Cuentas</h2>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <!-- Default box -->
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Cuenta</h3>

                    </div>
                    <div class="card-body">

                        <div class="row justify-content-end">

                            <div class="form-group">
                                <a href="@Url.Action("ReporteCuenta", "Cuenta")" class="btn btn-primary" style="margin-right:10px;"> <i class=" fas fa-print"></i>  </a>

                            </div>

                            <div class="form-group">

                                <button class="btn btn-danger form-group  fas fa-trash-alt" style="margin-right:10px;" onclick="ModalEliminarCuenta()"> </button>
                            </div>


                            <div class="form-group">
                                <button class="btn btn-secondary form-group fas fa-edit" style="margin-right:10px;" onclick="ObtenerCuenta()"> </button>
                            </div>


                            <div class="form-group">
                                <button class="btn btn-info" onclick="AbrirModalRegistro()" style="margin-right:10px;"> <i class=" fas fa-plus-square"></i> Registrar Cuenta </button>
                            </div>

                            <div class="form-group">
                                <button class="btn btn-primary" onclick="AbrirModalConfiguracion()"> <i class=" fas fa-cog"></i> Configurar Integracion </button>
                            </div>

                        </div>

                        <br />

                        <div style="width:700px; height:400px; overflow: scroll;">
                            <div id="tree">

                            </div>
                        </div>





                    </div>
                    <!-- /.card-body -->
                    <div class="card-footer">

                    </div>
                    <!-- /.card-footer-->
                </div>
                <!-- /.card -->
            </div>
        </div>
    </div>
</section>

<!-- Modal Structure -->
<div class="modal" id="modalcuenta" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="tituloEmpresa"></h5>
            </div>
            <form role="form">
                <div class="modal-body">

                    <div class="row">

                        <div class="col-md-12 form-group alert alert-danger" id="mensajeModal" style="display:none">

                        </div>

                        <input type="text" class="form-control" id="itIdGestion" hidden>

                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Nombre</label>
                                <input type="text" class="form-control" id="itNombre" placeholder="" maxlength="100">
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="RegistroCuenta()">Guardar</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="modalEditar">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="tituloeditar"></h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <form role="form">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 form-group alert alert-danger" id="mensajeModal" style="display:none">

                        </div>
                        <input type="text" class="form-control" id="itIdGestion" hidden>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Nombre</label>
                                <input type="text" class="form-control" id="itNombreEditar" placeholder="" maxlength="100">
                            </div>
                        </div>
                    </div>
                </div>
            </form>

            <div class="modal-footer justify-content-between">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                <button type="submit" class="btn btn-primary" onclick="EditarCuenta()">Guardar Cambios</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="modalEliminar">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Confirmacion</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input type="text" class="form-control" id="inputIdGestion" hidden>
                <p id="texto"></p>
            </div>
            <div class="modal-footer ">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-primary" onclick="EliminarCuenta()">Aceptar</button>
            </div>
        </div>
    </div>
</div>

<div class="modal" id="modalconfiguracion" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Configuracion de Integracion</h5>
            </div>
            <form role="form">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 form-group alert alert-danger" id="mensajeModal" style="display:none">

                        </div>

                        <input type="text" class="form-control" id="IdEmpresa" hidden>

                        <div class="col-md-12">

                            <div class="form-group">
                                <label for="radio" class="col-sm-4 col-form-label">Integracion </label>
                                <input name="integracion" type="radio" id="si" value="1" checked />
                                <label for="si"> Si</label>
                                <input name="integracion" type="radio" id="no" value="0" />
                                <label for="no"> No</label>
                            </div>

                            <div class="form-group row">
                                <label for="caja" class="col-sm-4 col-form-label">Caja:</label>
                                <div class="col-sm-8">
                                    <select class="form-control select2" id="caja">
                                        <option value="0">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="creaditofiscal" class="col-sm-4 col-form-label">Credito fiscal:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="creditofiscal">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="debitofiscal" class="col-sm-4 col-form-label">Debito Fiscal:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="debitofiscal">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="compras" class="col-sm-4 col-form-label">Compras:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="compras">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="ventas" class="col-sm-4 col-form-label">Ventas:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="ventas">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="it" class="col-sm-4 col-form-label">IT:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="it">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="itxpagar" class="col-sm-4 col-form-label">It x Pagar:</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="itxpagar">
                                        <option value="">Seleccione una Cuenta</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="RegistrarConfiguracion()">Guardar</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>

